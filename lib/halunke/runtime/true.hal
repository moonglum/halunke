(Class new 'True
  methods @[
    "and" { |'self 'other|
      other
    }
    "or" { |'self 'other|
      self
    }
    "then else" { |'self 'true_branch 'false_branch|
      (true_branch call [])
    }
    "to_s" { |'self 'other|
      "true"
    }
    "inspect" { |'self 'other|
      "true"
    }
  ]
)

('true = (True new))
